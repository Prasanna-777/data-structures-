    scanf("xd");
    printf("enter the number of columns =");
    scanf("%d"&c);
    print("enter the first matrix element=\n");
    for(i=0;i<r;i++)
    {
    	for(j=0;j<cj++)
    	{
    		scanf("%d" ,8a[i][j]);
        }
    }
